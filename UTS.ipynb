def masuk(jenis, produk):
    list_of_masuk=[]
    
    if produk in jenis:
        list_of_masuk=jenis[produk]
        for e in jenis[produk]:
            for i in masuk(jenis, e):
                if i not in list_of_masuk:
                    list_of_masuk+=masuk(jenis,e)
    else:
        list_of_masuk+=[]
        
    return list_of_masuk

